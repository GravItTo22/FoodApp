      {
        "type": "text",
        "name": "market"
      }
    ]
  },
  "exec": {
    "steps": {
      "name": "",
      "module": "core",
      "action": "trycatch",
      "options": {
        "try": {
          "steps": {
            "name": "GetProd",
            "module": "dbupdater",
            "action": "custom",
            "options": {
              "connection": "ClientApp",
              "sql": {
                "query": "SELECT mp.id, mp.prod_id, mp.rest_id, mp.prod_mark_id, mp.prod_name_en, mp.prod_name_bn, mp.prod_name_hin, mp.prod_img1, (select array_to_json(array_agg(row_to_json(q))) from (select rpt.id as price_id, rpt.quantity, rpt.q_unit, rpt.mrp, rpt.selling_price as price, rpt.product_tax from r_price_table rpt where rpt.prod_id=mp.id) q) as price FROM m_product mp left join r_price_table rpt on (rpt.prod_id=mp.id) WHERE mp.status=1 AND mp.market_id=@market AND mp.rest_id=@rest_id ORDER by mp.id desc LIMIT @limit",
                "params": [
                  {
                    "name": "@market",
                    "value": "{{$_GET.market}}",
                    "test": "1"
                  },
                  {
                    "name": "@rest_id",
                    "value": "{{$_GET.rest_id}}",
                    "test": "3"
                  },
                  {
                    "name": "@limit",
                    "value": "{{$_GET.limit}}",
                    "test": "2"
                  }
                ]
              }
            },
            "output": true,
            "meta": [
              {
                "name": "id",
                "type": "number"
              },
              {
                "name": "prod_id",
                "type": "text"
              },
              {
                "name": "rest_id",
                "type": "number"
              },
              {
                "name": "prod_mark_id",
                "type": "number"
              },
              {
                "name": "prod_name_en",
                "type": "text"
              },
              {
                "name": "prod_name_bn",
                "type": "text"
              },
              {
                "name": "prod_name_hin",
                "type": "text"
              },
              {
                "name": "prod_img1",
                "type": "text"
              },
              {
                "name": "price",
                "type": "array",
                "sub": [
                  {
                    "name": "price_id",
                    "type": "number"
                  },
                  {
                    "name": "quantity",
                    "type": "text"
                  },
                  {
                    "name": "q_unit",
                    "type": "text"
                  },
                  {
                    "name": "mrp",
                    "type": "text"
                  },
                  {
                    "name": "price",
                    "type": "text"
                  },
                  {
                    "name": "product_tax",
                    "type": "text"
                  }
                ]
              }
            ],
            "outputType": "array"
          }
        },
        "catch": {
          "steps": {
            "name": "Resp",
            "module": "core",
            "action": "setvalue",
            "options": {
              "value": "Suspicious Activity"
            },
            "meta": [],
            "output": true,
            "outputType": "text"
          }
        }
      }
    }
  }
